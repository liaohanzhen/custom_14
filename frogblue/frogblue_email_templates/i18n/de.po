#. module: frogblue_email_templates
#: model:mail.template,body_html:frogblue_email_templates.frogblue_email_template_edi_purchase
msgid ""
"\n"
"                    <!doctype html>\n"
"                    <html>\n"
"                        <head>\n"
"                            <meta charset=\"UTF-8\">\n"
"                            <title>Purchase Order/RFQ</title>\n"
"                        </head>\n"
"                        <body>\n"
"                            <% set lang = object.get_partner_lang() %>\n"
"                            <table width=\"100%\" border=\"0\" align=\"left\" cellpadding=\"0\" cellspacing=\"0\" style=\"color: #000000; font-family: 'Lucida Grande', 'Lucida Sans Unicode', 'Lucida Sans', 'DejaVu Sans', Verdana, sans-serif; font-size: 12px; background-color: #FFFFFF; margin-left: 0px; margin-top: 0px; margin-right: 0px; margin-bottom: 0px;\">\n"
"                                <tbody>\n"
"                                    <tr>\n"
"                                        <td align=\"left\" valign=\"top\" bgcolor=\"#ffffff\">\n"
"                                            <p>\n"
"                                                Dear\n"
"                                                % if object.partner_id and not object.partner_id.is_company:\n"
"                                                    % if object.partner_id.title:\n"
"                                                        ${object.partner_id.title.name or ''}\n"
"                                                    % endif\n"
"                                                    ${object.partner_id.name},\n"
"                                                % else:\n"
"                                                    Sir/Madam,\n"
"                                                % endif\n"
"                                            </p>\n"
"                                            <div style=\"line-height:1;\"> &nbsp;</div>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                    <tr>\n"
"                                        <td>\n"
"                                            <p>\n"
"                                                % if object.state in ['draft', 'sent', 'to approve']:\n"
"                                                    Please find attached our request no. ${object.name or ''}. <br/> <br/>\n"
"\n"
"                                                    Please send us your offer by ${object.get_formated_date(date=object.date_order)}. <br/> <br/>\n"
"                                                % else:\n"
"                                                    Please find attached our order no. ${object.name or ''} dated ${object.get_formated_date(date=object.date_order)}. <br/> <br/>\n"
"\n"
"                                                    Our preferred delivery date is as specified. <br/> <br/>\n"
"                                                % endif\n"
"\n"
"                                                % if object.state not in ['draft', 'sent', 'to approve']:\n"
"                                                    We would be grateful if you could send us a written order confirmation. <br/> <br/>\n"
"                                                % endif\n"
"\n"
"                                                If you have any questions, don't hesitate to contact me by phone on\n"
"                                                % if object.env and object.env.user and object.env.user.partner_id and object.env.user.partner_id.phone\n"
"                                                    ${object.env.user.partner_id and object.env.user.partner_id.phone or ''}\n"
"                                                % endif\n"
"                                                or e-mail. <br/> <br/>\n"
"\n"
"                                                Best regards, <br/> <br/>\n"
"                                                % if object.state in ['draft', 'sent', 'to approve']:\n"
"                                                    The ${object.company_id and object.company_id.name or ''} team\n"
"                                                % else:\n"
"                                                    ${object.env and object.env.user and object.env.user.name or ''} <br/> <br/>\n"
"                                                    % if object.env and object.env.user and object.env.user.title:\n"
"                                                        ${object.env.user.title.name or ''}\n"
"                                                    % endif\n"
"                                                % endif\n"
"                                            </p>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                    % if object.company_id and object.company_id.use_email_footer and object.company_id.email_footer:\n"
"                                        <tr>\n"
"                                            <td>\n"
"                                                <p>\n"
"                                                    % for line in object.company_id.email_footer.splitlines()\n"
"                                                        ${line}\n"
"                                                        <br />\n"
"                                                    % endfor\n"
"                                                </p>\n"
"                                            </td>\n"
"                                        </tr>\n"
"                                    % endif\n"
"                                </tbody>\n"
"                            </table>\n"
"                        </body>\n"
"                    </html>\n"
"                "
msgstr ""
"\n"
"                    \n"
"                    \n"
"                        \n"
"                            <meta charset=\"UTF-8\">\n"
"                            <title>Anfrage/Bestellung</title>\n"
"                        \n"
"                        \n"
"                            <% set lang = object.get_partner_lang() %>\n"
"                            \n"
"                                    % if object.company_id and object.company_id.use_email_footer and object.company_id.email_footer:\n"
"                                        \n"
"                                    % endif\n"
"                                <table width=\"100%\" border=\"0\" align=\"left\" cellpadding=\"0\" cellspacing=\"0\" style=\"background-color:transparent;border-collapse:collapse;color: #000000; font-family: 'Lucida Grande', 'Lucida Sans Unicode', 'Lucida Sans', 'DejaVu Sans', Verdana, sans-serif; font-size: 12px; background-color: #FFFFFF; margin-left: 0px; margin-top: 0px; margin-right: 0px; margin-bottom: 0px;\">\n"
"                                <tbody>\n"
"                                    <tr>\n"
"                                        <td align=\"left\" valign=\"top\" bgcolor=\"#ffffff\" style=\"padding:0px;\">\n"
"                                            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">\n"
"                                                Sehr geehrte(r)\n"
"                                                % if object.partner_id and not object.partner_id.is_company:\n"
"                                                    % if object.partner_id.title:\n"
"                                                        ${object.partner_id.title.name or ''}\n"
"                                                    % endif\n"
"                                                    ${object.partner_id.name},\n"
"                                                % else:\n"
"                                                    Damen und Herren,\n"
"                                                % endif\n"
"                                            </p>\n"
"                                            <div style=\"font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;line-height:1;\"> &nbsp;</div>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                    <tr>\n"
"                                        <td style=\"padding:0px;\">\n"
"                                            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">\n"
"                                                % if object.state in ['draft', 'sent', 'to approve']:\n"
"                                                    In der Anlage erhalten Sie unsere Anfrage Nr. ${object.name or ''}.&nbsp;<br>&nbsp;<br>\n"
"\n"
"                                                    Bitte senden Sie uns bis zum ${object.get_formated_date(date=object.date_order)} Ihr Angebot zu. <br>&nbsp;<br>\n"
"                                                % else:\n"
"                                                    anbei erhalten Sie unser Bestellung Nr. ${object.name or ''} vom ${object.get_formated_date(date=object.date_order)}.&nbsp;<br>&nbsp;<br>\n"
"\n"
"                                                    Bitte beachten Sie unseren gewünschten Liefertermin.&nbsp;<br>&nbsp;<br>\n"
"                                                % endif\n"
"\n"
"                                                % if object.state not in ['draft', 'sent', 'to approve']:\n"
"                                                    Bitte senden Sie uns eine schriftliche Auftragsbestätigung zu.<br>&nbsp;<br>\n"
"                                                % endif\n"
"\n"
"                                                Sollten Sie Fragen haben, erreichen Sie mich telefonisch unter\n"
"                                                % if object.env and object.env.user and object.env.user.partner_id and object.env.user.partner_id.phone\n"
"                                                    ${object.env.user.partner_id and object.env.user.partner_id.phone or ''}\n"
"                                                % endif\n"
"                                                oder via Email.&nbsp;<br>&nbsp;<br>\n"
"\n"
"                                                Mit freundlichen Grüßen,&nbsp;<br>&nbsp;<br>\n"
"                                                % if object.state in ['draft', 'sent', 'to approve']:\n"
"                                                    Ihr ${object.company_id and object.company_id.name or ''} Team\n"
"                                                % else:\n"
"                                                    ${object.env and object.env.user and object.env.user.name or ''}&nbsp;<br>&nbsp;<br>\n"
"                                                    % if object.env and object.env.user and object.env.user.title:\n"
"                                                        ${object.env.user.title.name or ''}\n"
"                                                    % endif\n"
"                                                % endif\n"
"                                            </p>\n"
"                                        </td>\n"
"                                    </tr><tr>\n"
"                                            <td style=\"padding:0px;\">\n"
"                                                <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">\n"
"                                                    % for line in object.company_id.email_footer.splitlines()\n"
"                                                        ${line}\n"
"                                                        <br>\n"
"                                                    % endfor\n"
"                                                </p>\n"
"                                            </td>\n"
"                                        </tr></tbody>\n"
"                            </table>\n"
"                        \n"
"                    \n"
"                "

#. module: frogblue_email_templates
