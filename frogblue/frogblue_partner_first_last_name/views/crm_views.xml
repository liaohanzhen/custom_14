<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="frogblue_crm_case_form_view_leads_partner_first_name_inherit" model="ir.ui.view">
            <field name="name">Add firstname and surnames</field>
            <field name="model">crm.lead</field>
            <field name="inherit_id" ref="crm.crm_lead_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[1]/group[2]/label" position="before">
                    <field name="lastname" string="Last Name"/>
                    <field name="firstname" string="First Name"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
