<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="product_supplierinfo_form_account_not_account_inherit" model="ir.ui.view">
		<field name="name">product.supplierinfo.form.account.not.account.inherit</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_form_view"/>
        <field name="arch" type="xml">
        	<field name="name" position="after">
        		<field name="contract_status" invisible="1"/>
        	</field>
        </field>
    </record>
    
    <record id="product_supplierinfo_tree_account_not_account_inherit" model="ir.ui.view">
		<field name="name">product.supplierinfo.tree.account.not.account.inherit</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_tree_view"/>
        <field name="arch" type="xml">
        	<field name="date_end" position="after">
        		<field name="contract_status" string="Account Status"/>
        	</field>
        </field>
    </record>
    
</odoo>