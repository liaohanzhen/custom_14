					<option value="Deployment partners (installers)">Deployment partners (installers)</option>
				</select>
				<div class="validate"></div>
			</div>

			<div class="form-group">
				<label for="b_message"><strong>Message*</strong></label>
				<textarea class="form-control" name="b_message" id="b_message" rows="5" data-rule="required" data-msg="Please write something for us" placeholder="Message"></textarea>
				<div class="validate"></div>
			</div>

			<div class="form-group">
				<label for="b_know_how"><strong>How do you know about us?</strong></label>
				<select name="b_know_how" id="b_know_how" class="form-control">
					<option value="">--Select source--</option>
					<!--<t t-foreach="source" t-as="source">
						<option t-attf-value="{{source.name}}"><t t-esc="source.name" /></option>
					</t>-->
					<option value="Search engine">Search engine</option>
					<option value="Social media">Social media</option>
					<option value="News">News</option>
					<option value="Industry event">Industry event</option>
					<option value="Referral">Referral</option>
					<option value="Word of mouth">Word of mouth</option>
					<option value="Other">Other (please specify)</option>
				</select>
			</div>
			<div class="form-group b_show_other" style="display:none">
				<label for="b_other_source"><strong>Other (please specify)</strong></label>
				<input type="text" class="form-control" name="b_other_source" id="b_other_source"  placeholder="Please specify" />
			</div>
			
			<div class="form-group">
				<input type="checkbox" checked="checked" id="b_check" name="b_check" />
				<label class="form-check-label" style="vertical-align:middle !important;" for="b_check">Yes, I would you like to hear about general 5B world-changing updates.</label>
			  </div>
			  
			<input type="hidden" value="become" name="form_type" />
			<div class="text-center"><button type="submit">Send message <img src="/theme_5b/static/img/loading1.gif" style="display:none" /></button></div>
			
			
		</form>
	</template>
	
	<template id="inherit_assets_frontend" name="web_example assets"  inherit_id="website.assets_frontend">
	    <xpath expr="script[last()]" position="after">
			<script type="text/javascript" src="/website_5b/static/src/js/script.js"/>
		</xpath>
	</template>

  </data>
</odoo>