<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="product_supplierinfo_form_approved_not_approved_inherit" model="ir.ui.view">
		<field name="name">product.supplierinfo.form.approved.not.approved.inherit</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_form_view"/>
        <field name="arch" type="xml">
        	<field name="name" position="after">
        		<field name="status" invisible="1"/>
        	</field>
        </field>
    </record>
    
    <record id="product_supplierinfo_tree_approved_not_approved_inherit" model="ir.ui.view">
		<field name="name">product.supplierinfo.tree.approved.not.approved.inherit</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_tree_view"/>
        <field name="arch" type="xml">
        	<field name="date_end" position="after">
        		<field name="status" />
        	</field>
        </field>
    </record>
    
</odoo>