<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for hospital.patient -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Hospital Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hospital.appointment -->
        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Hospital Appointment</field>
            <field name="code">hospital.appointment.sequence</field>
            <field name="prefix">HA</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hospital.doctor -->
        <record id="seq_hospital_doctor" model="ir.sequence">
            <field name="name">Hospital Doctor</field>
            <field name="code">hospital.doctor.sequence</field>
            <field name="prefix">HD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
